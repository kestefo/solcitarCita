<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="com.rava.project.controller.Page1" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog id="DialogConfirmarRegistro" horizontalScrolling="true" showHeader="false" state="None" stretch="false" title="Actualizar Usuario" type="Standard"
		verticalScrolling="true" class="sapUiResponsiveContentPadding">
		<content>
			 <Text text="DNI NO REGISTRADO, DESEA REGISTRARLO?" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
		</content>
		<buttons>
			<Button text="Ok" type="Accept" iconFirst="true" width="auto" enabled="true" visible="true"
				iconDensityAware="false" press="responseConf"/>
			<Button text="Cancelar" type="Reject" iconFirst="true" width="auto" enabled="true" visible="true"
				iconDensityAware="false" press="CloseDialogConfRegDni"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>